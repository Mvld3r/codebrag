// diff
.diff
  margin-top: -1px;
  padding: 0 0.77em 0 0;
  &.opened
    position: absolute;
    right: 0;
    width: 100%;
    transition: width 1000ms;
    z-index: 2;

    & .toggle-diff-container-btn:hover i
      margin-left: 12px;


.diff-header
  padding: 0.77em 3em 0.77em 2.3em;

  & label[for="file-changer"]
    display: inline-block;
    float: left;
    margin: 1px 0 0;
    padding: 0.6em;
    text-transform: uppercase;

  & .file-changer
    background: #e7e0ce;
    border: 1px solid #e7e0ce;
    color: $second-body-text-color;
    float: left;
    max-width: 20em;
    margin: 1px 0 0;
    padding: 0.38em;
    outline: none;
    & option
      background: white;

  & .accept.button, 
  .secondary.button
    margin: 0;

  & .secondary.button
    cursor: default;
    &:hover
      background: #cdc5ac;
      box-shadow: none;
    & i
      cursor: default;


.diff-wrapper
  height: 100%;
  background: #fff;
  border: 1px solid #e4d9b8;
  box-shadow: 0 0 7px 0 rgba(211,196,160,0.7);
  margin-left: -1px;
  overflow-y: auto;


.diff-container
  padding: 2.3em 1.15em 1.15em 2.3em;

  & .reviewers-info
    float: right;
    padding: 0.4em;
    background: #fcfaad;
    border: 1px solid #F8F586;
    color: #f9ac12;

    .reviewers-list
      list-style: none
      margin: 0
      padding: 0

  & .authored-box
    margin-bottom: 2.3em;

    & img
      margin-right: 1.15em;
      width: 3.85em;

    & .commit-name
      font-weight: bold;
      line-height: 1.3;
      margin-bottom: 0.4em;


.toggle-diff-container-btn
  background: white;
  border: 1px solid #e4d9b8;
  border-right: none;
  bottom: 0;
  cursor: pointer;
  left: 0;
  opacity: 0;
  position: absolute;
  top: 0;
  width: 2.2em;
  z-index: 5;
  transition: opacity 250ms, width 500ms, left 500ms;
  &:hover
    left: -0.77em;
    opacity: 1;
    width: 2.9em;

  & i
    color: $main-orange-color;
    margin-left: 5px;
    position: absolute;
    top: 45%;


.diff-table-wrapper
  border: 1px solid $main-border-color;
  margin-bottom: 2.3em;
  overflow-x: auto;
  overflow-y: hidden;


.diff-file-container
  border: none;
  font-family: Consolas, "Liberation Mono", Courier, monospace;
  margin: 0;
  width: 100%;

  & thead
    background: $body-bg-color;
    border-bottom: 1px solid $main-border-color;

    & th
      color: $body-text-color;
      font-size: 1em;
      font-weight: normal;
      padding: 0.77em 1.15em;
      text-align: left;

  & tbody
    font-size: 13px;

  & .diff-line-code
    white-space: pre;
    cursor: pointer;

  & .diff-line-num
    border-right: 1px solid #d6d6d6;
    line-height: 1.125em;
    margin: 0;
    max-width: 50px;
    min-width: 50px;
    padding: 0;
    text-align: right;
    width: 50px;

    & .button
      background: transparent;
      border: none;
      box-shadow: none;
      height: 100%;
      line-height: inherit;
      margin: 0;
      padding: 0;
      position: relative;
      width: 100%;
      &.comment
        background: transparent;

    & .diff-line-num-content
      color: #aaaaaa;
      display: inline-block;
      margin: 0.38em 0.77em;
      height: 14px;
      min-width: 1em;
      vertical-align: middle;

      & span
        display: inline-block;
        min-height: 14px;

    & i
      visibility: hidden;
      position: absolute;
      left: 50%;
      margin-left: -0.5em;
      top: 50%;
      margin-top: -0.5em;

  & tr.removed
    background: #ffdddd;

    & .diff-line-num
      background: #f7c8c8;
      border-color: #e9aeae;

    & .diff-line-num-content
      color: #bd8f8f;

  & tr.added
    background: #d4f7c1;

    & .diff-line-num
      background: #bce7a5;
      border-color: #b0cfa3;

    & .diff-line-num-content
      color: #95a88d;

  & tr.header
    background: #efefef;

    & .diff-line-num
      background: #ededed;
      border-color: #cdcdcd;
      color: #ababab;
      
  & tr.comments-container
    background: $body-bg-color;
    border-left: none;
    border-right: none;
    border-bottom: none;

    & td
      padding: 1.15em 1.15em 0;


.diff-file-container tr.liked-by-others
  & .like.button
    background: #f3720b;

    & .icon-heart
      color: rgb(255, 255, 255);
      visibility: visible;

    & .diff-line-num-content
      visibility: hidden;


.diff-file-container tr.liked-by-user
  & .like.button
    background: #f3720b;
    & .icon-heart
      color: rgb(185, 80, 21);
      visibility: visible;
    & .diff-line-num-content
      visibility: hidden;


tbody tr.comments-container:last-child
  border-bottom: 1px solid $main-border-color;


tr:hover .diff-line-num .button
  background: $main-orange-color;
  &:hover
    background: #c25b09;
  &.comment
    background: $green-button-bg-color;
    &:hover
      background: $green-button-bg-hover;
  & .diff-line-num-content
    visibility: hidden
  & i
    visibility: visible;


.diff-not-displayed
  margin-bottom: 2.3em;

  & ul
    margin: 0;
    padding: 0;

  & li
    list-style: none;

  & h6
    color: $main-orange-color;
    font-size: 1em;
    font-weight: normal;
    margin: 0 0 0.38em 0;
    text-transform: uppercase;

.diff-not-displayed-file-name
  font-family: Consolas, "Liberation Mono", Courier, monospace;
  line-height: 1.4;
  list-style: none;
  
.diff-file-name
	& a
		text-decoration: none;
		color: #2f0200;	
		&:hover
			text-decoration: underline;

.diff
  & .popover
    position: absolute;
    top: 0;
    left: 0;
    z-index: 100;
    background-color: #fff;
    border-radius: 4px;
    border: 1px solid #d1cbb9;
    box-shadow(0 5px 10px rgba(0,0,0,.2));
    text-align: left;
    & > .popover-title
    	font-weight: bold;
    	background: #f7f5ef;
    	margin: 0;
    	padding: 8px 14px;
    	font-size: 14px;
    	background-color: #f7f7f7;
    	border-bottom: 1px solid #ebebeb;
    	border-radius: 5px 5px 0 0;
    & > .popover-content
        padding: 9px 14px;
        min-width: 400px;
    & > .arrow
        &, &:after
            position: absolute;
            display: block;
            width: 0;
            height: 0;
            border-color: transparent;
            border-style: solid;
    & > .arrow
        border-width: 6px;
    & > .arrow:after
        border-width: 6px;
        content: "";
    & > .arrow
        top: 50%;
        left: -6px;
        margin-top: -6px;
        border-left-width: 0;
        border-right-color: #f7f5ef;
        &:after
            content: " ";
            left: 1px;
            border-left-width: 0;
            border-right-color: #f7f5ef;
            bottom: -6px;

.diff-blame
	cursor: pointer; 


.diff-not-displayed-stat
  color: $second-body-text-color;
  font-family: 'PT Sans', Tahoma, sans-serif;
  font-size: 0.814em;


.like-commit
  background: $body-bg-color;
  color: $second-body-text-color;
  display: inline-block;
  margin-bottom: 0.38em;
  padding-right: 1.15em;
  cursor: pointer;

  & .like.button
    padding: .3em 1.15em 0.25em;
    margin: 0;

  & span
    display: inline-block;
    margin-top: .2em;
    vertical-align: top;

  &.liked-by-others .like.button
    background: #f3720b;
    & .icon-heart
      color: rgb(255, 255, 255);
      
  &.liked-by-user .like.button
    background: #f3720b;
    & .icon-heart
      color: rgb(185, 80, 21);
  & .user-who-like:after
    content ",\0000a0";



// Inline likes
.diff-file-container tr.inline-likes
  background: $body-bg-color;
  color: $main-orange-color;
  padding: 0;
  & td
    padding: 0.38em 0.38em 0.38em 1.15em;
    & .comment
      margin-bottom: 0;

.likes-list
  display: inline;
  list-style: none;
  padding: 0;
  & li
    display: inline;
    margin-right: 5px;
    white-space: nowrap;
    &:after
      content:',';
      position: relative;
  & li:last-child:after
    content:'';

.likes
  color: $main-orange-color;

.scroll-target-reaction-mark
  background: $main-border-color;

.scroll-target-reaction-done
  background: $body-bg-color;
  transition: background 1.5s linear;

#followups-tab-content .diff-container
  padding: 2.3em 1.15em 2em 2.3em;

.diff-loading
  text-align: center;
  padding-top: 1em;
  padding-bottom: 1em;

